<% unless @inactive_camps.empty? %>
  <div class="panel clearfix">
    <h5>Inactive Camps</h5>
    <ul class="side-nav">
      <% @inactive_camps.each do |camp| %>
        <li><%= link_to "#{camp.name}: #{camp.start_date.strftime('%m/%d/%y')} at #{camp.location.name}", camp_path(camp) %></li>
      <% end %>
    </ul>
  </div>
<% end %>