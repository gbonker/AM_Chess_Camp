<% unless @registrations.empty? %>
  <!-- <h3>&nbsp;</h3> -->
  <div class="panel clearfix">
    <h5>Student's Registrations:</h5>
    <ul class="side-nav">
      <% @registrations.each do |registration| %>
        <li><%="#{registration.camp.start_date.strftime('%B %e, %Y')}: #{registration.camp.name} at #{registration.camp.location.name}" %></li>
      <% end %>
    </ul>
  </div>

<% else %>
  <div class="panel clearfix">
    <h5>This student has not registered for any camps yet.</h5>
  </div>
<% end %>